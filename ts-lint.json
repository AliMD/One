{
  "compilerOptions": {
    "strict": true,
    "lib": ["ScriptHost", "WebWorker", "DOM", "DOM.Iterable"],
    "target": "ES2019",
    "module": "ES2020",
    "outDir":"./dist",
    "declaration": true,
    "skipLibCheck": true,
    "importHelpers": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "noUnusedParameters": true,
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  }
}
